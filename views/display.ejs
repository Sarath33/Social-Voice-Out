
<!DOCTYPE html> 
<html lang="en"> 
  
<head> 
    <meta charset="UTF-8"> 
    <meta name="viewport" content="width=device-width, initial-scale=1.0"> 
    <title>Image Uploading</title> 
</head> 
  
<body> 
    
  
    <h1>Uplaoded Images</h1> 
    <% obj.forEach(function(objj){ %>
    <div class="thumbnail">
        <img class="img-responsive" src="data:image/<%=objj.img.contentType%>;base64, <%=objj.img.data.toString('base64')%>" width="100%" height="100%" >
    </div>
    <% }) %>
</body> 
  
</html> 
